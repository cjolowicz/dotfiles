{
  "agent": {
    "default_model": {
      "provider": "anthropic",
      "model": "claude-3-7-sonnet-thinking-latest"
    },
    "version": "2"
  },
  "buffer_font_size": 13,
  "cursor_blink": false,
  "git": {
    "inline_blame": {
      "enabled": true,
      "show_commit_summary": true
    }
  },
  "project_panel": {
    "dock": "left"
  },
  "scrollbar": {
    "show": "never"
  },
  "tab_bar": { "show": false },
  "terminal": {
    "font_family": "FiraCode Nerd Font Mono"
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false
  },
  "ui_font_size": 14,
  "vim_mode": true
}
